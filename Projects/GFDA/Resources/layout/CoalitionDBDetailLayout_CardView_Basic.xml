<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:cardview="http://schemas.android.com/apk/res-auto">
<androidx.cardview.widget.CardView
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
	android:visibility="gone"
    android:id="@+id/CoalitionDBDetailBasicInfoCardLayout"
    cardview:cardElevation="4dp"
    cardview:cardCornerRadius="5dp"
    cardview:cardBackgroundColor="@color/background_material_dark">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/CoalitionDBDetailBasicInfoLayout">
    <!--Coalition Basic Info - Born Grade Part-->
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/Common_BornGrade"
                android:gravity="center" />
            <include layout="@layout/seperatebar_vertical"/>
            <TextView
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:gravity="center"
                android:id="@+id/CoalitionDBDetailInfoBornGrade"/>
        </LinearLayout>
    <!--Coalition Basic Info - Type Part-->
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/Common_Type"
                android:gravity="center" />
            <include layout="@layout/seperatebar_vertical"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/CoalitionDBDetailInfoType"
                android:gravity="center" />
        </LinearLayout>
    <!--Coalition Basic Info - Name Part-->
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/Common_Name"
                android:gravity="center" />
            <include layout="@layout/seperatebar_vertical"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/CoalitionDBDetailInfoName"
                android:gravity="center" />
        </LinearLayout>
    <!--Coalition Basic Info - NickName Part-->
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/Common_NickName"
                android:gravity="center" />
            <include layout="@layout/seperatebar_vertical"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/CoalitionDBDetailInfoNickName"
                android:gravity="center" />
        </LinearLayout>
    <!--Coalition Basic Info - Affiliation Part-->
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/Common_Affiliation"
                android:gravity="center" />
            <include layout="@layout/seperatebar_vertical"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/CoalitionDBDetailInfoAffiliation"
                android:gravity="center" />
        </LinearLayout>
    <!--Coalition Basic Info - Illustrator Part-->
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/Common_Illustrator"
                android:gravity="center" />
            <include layout="@layout/seperatebar_vertical"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/CoalitionDBDetailInfoIllustrator"
                android:gravity="center" />
        </LinearLayout>
    <!--Coalition Basic Info - VoiceActor Part-->
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/Common_VoiceActor"
                android:gravity="center" />
            <include layout="@layout/seperatebar_vertical"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/CoalitionDBDetailInfoVoiceActor"
                android:gravity="center" />
        </LinearLayout>
    <!--Coalition Basic Info - Gain Part-->
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/Common_HowToGain"
                android:gravity="center" />
            <include layout="@layout/seperatebar_vertical"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/CoalitionDBDetailInfoHowToGain"
                android:gravity="center" />
        </LinearLayout>
    </LinearLayout>
</androidx.cardview.widget.CardView>
</merge>
