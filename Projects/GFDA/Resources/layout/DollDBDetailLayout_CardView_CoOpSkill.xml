<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:cardview="http://schemas.android.com/apk/res-auto">
<androidx.cardview.widget.CardView
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
    android:visibility="gone"
    android:id="@+id/DollDBDetailCoOpSkillCardLayout"
    cardview:cardElevation="4dp"
    cardview:cardCornerRadius="5dp"
    cardview:cardBackgroundColor="@color/background_material_dark">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
		android:layout_marginTop="10dp"
		android:layout_marginBottom="10dp"
        android:id="@+id/DollDBDetailCoOpSkillLayout">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textStyle="bold"
            android:text="@string/DollDBDetail_CoOpSkillTitle"
            android:gravity="center"
            android:id="@+id/DollDBDetailCoOpSkillTitle" />
    <!--Doll Co-Op Skill Part - Basic Layout-->
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/DollDBDetailCoOpSkillTitle"
            android:id="@+id/DollDBDetailCoOpSkillBasicLayout">
        <!--Doll Co-Op Skill Part - Basic Layout - Skill Icon-->
            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:id="@+id/DollDBDetailCoOpSkillIcon"
                android:layout_margin="5dp" />
        <!--Doll Co-Op Skill Part - Basic Layout - Skill Name-->
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="70dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:gravity="center"
                android:id="@+id/DollDBDetailCoOpSkillName" />
        <!--Doll Co-Op Skill Part - Basic Layout - Skill CoolTime-->
        </LinearLayout>
    <!--Doll Co-Op Skill Part - Skill Explain-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:gravity="center"
            android:layout_below="@id/DollDBDetailCoOpSkillBasicLayout"
            android:id="@+id/DollDBDetailCoOpSkillExplain" />
    <!--Doll Co-Op Skill Part - Skill Ability-->
    </RelativeLayout>
</androidx.cardview.widget.CardView>
</merge>
